    <Authors>Omer Tamir</Authors>
    <Company></Company>
    <PackageProjectUrl>https://github.com/omermina/ETradeApiV1</PackageProjectUrl>
    <PackageTags>E*Trade  V1 API</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/omermina/ETradeApiV1</RepositoryUrl>
    <PackageId>ETrade-V1Api</PackageId>
    <Version>1.0.14</Version>
    <AssemblyVersion>1.0.1.6</AssemblyVersion>
    <FileVersion>1.0.1.6</FileVersion>
    <Description>Simple REST client to connect to E*Treade V1 new API</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="2.2.0" />
    <PackageReference Include="RestSharp" Version="106.6.9" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.5.0" />
  </ItemGroup>

</Project>
